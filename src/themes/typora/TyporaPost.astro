---
import { renderMarkdownToHtml } from '../../utils/markdown';

interface Props {
  content?: string;
  encryptedHtml?: string;
}

const { content = '', encryptedHtml } = Astro.props;
const html = encryptedHtml ? '' : renderMarkdownToHtml(content);
---

{encryptedHtml ? <div data-typora-aes={encryptedHtml} /> : <div set:html={html} />}
